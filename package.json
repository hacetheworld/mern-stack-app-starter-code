    "url": "https://github.com/hacetheworld/mern-stack-app-starter-code/issues"
  },
  "homepage": "https://github.com/hacetheworld/mern-stack-app-starter-code#readme",
  "dependencies": {
    "concurrently": "^5.1.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.2"
  }
}